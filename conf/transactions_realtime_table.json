{
  "tableName": "transactions",
  "tableType": "REALTIME",

  "segmentsConfig": {
    "replication": "1",
    "timeColumnName": "create_dt"
  },

  "validationConfig": {
    "schemaName": "transactions"
  },

  "tenants": {
    "broker": "DefaultTenant",
    "server": "DefaultTenant"
  },

  "tableIndexConfig": {
    "invertedIndexColumns": ["label", "country_code", "receiving_country", "payment_method"],
    "rangeIndexColumns": ["deposit_amount"],
    "bloomFilterColumns": ["user_seq"],
    "loadMode": "HEAP",
    "nullHandlingEnabled": true,

    "streamConfigs": {
      "streamType": "kafka",
      "stream.kafka.consumer.type": "lowLevel",
      "stream.kafka.topic.name": "transactions_rt",
      "stream.kafka.broker.list": "93.115.172.151:9092",
      "stream.kafka.decoder.class.name": "org.apache.pinot.plugin.stream.kafka.KafkaJSONMessageDecoder",
      "stream.kafka.consumer.prop.auto.offset.reset": "smallest",
      "realtime.segment.flush.threshold.rows": "1000",
      "realtime.segment.flush.threshold.time": "1d"

    }
  },

  "routing": {},
  "metadata": {}
